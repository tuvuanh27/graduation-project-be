      }
    },
    "queue": {
      "type": "library",
      "root": "libs/queue",
      "entryFile": "index",
      "sourceRoot": "libs/queue/src",
      "compilerOptions": {
        "tsConfigPath": "libs/queue/tsconfig.lib.json"
      }
    },
    "redis": {
      "type": "library",
      "root": "libs/redis",
      "entryFile": "index",
      "sourceRoot": "libs/redis/src",
      "compilerOptions": {
        "tsConfigPath": "libs/redis/tsconfig.lib.json"
      }
    },
    "web3": {
      "type": "library",
      "root": "libs/web3",
      "entryFile": "index",
      "sourceRoot": "libs/web3/src",
      "compilerOptions": {
        "tsConfigPath": "libs/web3/tsconfig.lib.json"
