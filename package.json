{
  "name": "boiler",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:ts": "nest start",
    "start": "node dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "postinstall": "husky install",
    "proto:gen": "npx protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto.cmd --ts_proto_out=. ./assets/proto/learning/learning.proto --ts_proto_opt=nestJs=true",
    "typechain": "npx typechain --target web3-v1 assets/abi/nft.abi.json --out-dir assets/abi"
  },
  "engines": {
    "node": "16.19.0",
    "npm": "8.19.3",
    "yarn": "1.22.19"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.8.12",
    "@grpc/proto-loader": "^0.7.5",
    "@mfsoftworks/nestjs-ipfs": "^1.0.2",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/bull": "^0.6.3",
    "@nestjs/cli": "9.2.0",
    "@nestjs/common": "9.3.12",
    "@nestjs/config": "2.3.1",
    "@nestjs/core": "9.3.12",
    "@nestjs/cqrs": "9.0.3",
    "@nestjs/mapped-types": "1.2.2",
    "@nestjs/microservices": "9.0.0",
    "@nestjs/mongoose": "9.2.0",
    "@nestjs/platform-express": "9.0.0",
    "@nestjs/schedule": "2.2.0",
    "@nestjs/swagger": "6.2.1",
    "@ntegral/nestjs-sentry": "^4.0.0",
    "@pinata/sdk": "^2.1.0",
    "@sentry/hub": "7.42.0",
    "@sentry/node": "7.42.0",
    "@types/bull": "^4.10.0",
    "axios": "^1.3.5",
    "bignumber.js": "^9.1.1",
    "bluebird": "^3.7.2",
    "bn.js": "^5.2.1",
    "buffer-to-stream": "^1.0.0",
    "bull": "^4.10.4",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cloudinary": "^1.35.0",
    "dotenv": "16.0.3",
    "fp-ts": "^2.13.1",
    "husky": "8.0.1",
    "ioredis": "^5.3.1",
    "ipfs": "0.66.0",
    "joi": "17.8.3",
    "kafkajs": "^2.2.4",
    "log4js": "6.8.0",
    "moment": "2.29.4",
    "mongoose": "6.6.5",
    "morgan": "1.10.0",
    "nanoid": "3.3.1",
    "nestjs-console": "8.0.0",
    "nestjs-request-context": "2.1.0",
    "nft.storage": "^7.0.3",
    "node-fetch": "2",
    "oxide.ts": "1.1.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.2.0",
    "tsimportlib": "^0.0.3",
    "web3": "^1.9.0"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "npm run format",
      "git add ."
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@nestjs/schematics": "9.0.0",
    "@nestjs/testing": "9.0.0",
    "@typechain/web3-v1": "^6.0.2",
    "@types/bluebird": "^3.5.38",
    "@types/bn.js": "^5.1.1",
    "@types/buffer-to-stream": "^1.0.0",
    "@types/express": "4.17.13",
    "@types/jest": "29.2.4",
    "@types/multer": "^1.4.7",
    "@types/node": "18.11.18",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "eslint": "8.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "29.3.1",
    "prettier": "2.3.2",
    "source-map-support": "0.5.20",
    "supertest": "6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "9.2.3",
    "ts-node": "10.0.0",
    "ts-proto": "^1.143.0",
    "tsconfig-paths": "4.1.1",
    "typechain": "^8.1.1",
    "typescript": "4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@libs/libs(|/.*)$": "<rootDir>/libs/libs/src/$1",
      "^@app/configs(|/.*)$": "<rootDir>/libs/configs/src/$1",
      "^@libs/grpc-client(|/.*)$": "<rootDir>/libs/grpc-client/src/$1",
      "^@queue/queue(|/.*)$": "<rootDir>/libs/queue/src/$1",
      "^@libs/redis(|/.*)$": "<rootDir>/libs/redis/src/$1",
      "^@libs/web3(|/.*)$": "<rootDir>/libs/web3/src/$1",
      "^@libs/database(|/.*)$": "<rootDir>/libs/database/src/$1",
      "^@libs/kafka(|/.*)$": "<rootDir>/libs/kafka/src/$1",
      "^@libs/ipfs-lib(|/.*)$": "<rootDir>/libs/ipfs-lib/src/$1"
    }
  }
}
